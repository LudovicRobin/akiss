#set xor;

// Signature xor

symbols plus/2, zero/0, c/0;

private a, b, ok;
channels C;
var X, Y, Z;

rewrite plus(zero,X) -> X;
rewrite plus(X,X) -> zero;

s1 = out(C,a).out(C,plus(a,b));
r1 = out(C,a).out(C,plus(a,c));

not equivalentct? s1 and r1;

