#set xor;

// Signature xor

symbols plus/2, zero/0, c/0;

private a, b, ok;
channels C;
var X, Y, Z;

rewrite plus(zero,X) -> X;
rewrite plus(X,X) -> zero;


s2 = out(C,plus(a,b)).out(C,plus(a,c)).out(C,b);
r2 = out(C,plus(a,b)).out(C,plus(a,c)).out(C,plus(b,c));

not equivalentct? s2 and r2;

